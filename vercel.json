{
  "framework": "svelte",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "build",
  "routes": [
    { "src": "/_app/immutable/.+", "headers": { "cache-control": "public,immutable,max-age=31536000" } },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
